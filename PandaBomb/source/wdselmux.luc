module wdselmux (
    input wdsel[2],  // 2 bit wdsel
    input alu,  // alu at 000
    input rd[16], //16 bit rd from rom
    output out // output from wdsel mux
  ) {

  always {
    case(wdsel){
    2b00: out=alu;
    2b01: out=hffff;
    2b10: out=h000f;
    2b11: out=h0;
    default: out=rd; //100 is more than 2 bit
  }
}
